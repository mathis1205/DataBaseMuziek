<Window x:Class="DataBaseMuziek.Overzicht"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataBaseMuziek"
        mc:Ignorable="d"
        Title="Overzicht" Height="200" Width="200">
    <Grid>
        <Button x:Name="btnMuziek" Content="Muziek" HorizontalAlignment="Center" Height="25" Margin="0,40,0,0" VerticalAlignment="Top" Width="120" Click="btnMuziek_Click"/>
        <Button x:Name="btnAlbum_Copy" Content="Album" HorizontalAlignment="Center" Height="25" Margin="0,10,0,0" VerticalAlignment="Top" Width="120" Click="btnAlbum_Copy_Click"/>
        <Button x:Name="btnZanger" Content="Zanger" HorizontalAlignment="Center" Height="25" Margin="0,70,0,0" VerticalAlignment="Top" Width="120" Click="btnZanger_Click"/>

    </Grid>
</Window>
